#include<stdio.h>
int main (){
int n,oddsum=0,evensum=0;
printf("enter the number:");
scanf("%d",&n);
for (int i = 0; i <=n; i++)
{
   if (i%2==0)
   {
   evensum=evensum+i;
   }
   


   else{
   oddsum=oddsum+i;
   }
}
printf("sum of odd=%d\n",oddsum);
printf("sum of even=%d",evensum);
 return 0;
}
